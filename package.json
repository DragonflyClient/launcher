{
  "name": "dragonfly-launcher",
  "productName": "Dragonfly Launcher",
  "version": "0.3.1",
  "description": "The launcher for the Dragonfly minecraft client",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "echo \"No linting configured\"",
    "build": "electron-builder build --win --l --mac --publish never",
    "build-linux": "electron-builder build --l --publish never",
    "build-mac": "electron-builder build --mac --publish never",
    "build-win": "electron-builder build --win --publish never",
    "publish": "electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DragonflyClient/launcher-releases.git"
  },
  "keywords": [],
  "author": {
    "name": "Dragonfly",
    "email": "support@playdragonfly.net"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dragonfly_launcher"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "crypto-js": "^4.0.0",
    "discord-rpc": "^3.1.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5",
    "fswin": "^3.19.908"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  }
}
